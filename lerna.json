{
  "$schema": "node_modules/lerna/schemas/lerna-schema.json",
  "command": {
    "publish": {
      "ignoreChanges": [
        "**/__fixtures__/**",
        "**/__mocks__/**",
        "**/__tests__/**",
        "CHANGELOG.md"
      ],
      "message": "release: publish npm[skip ci]"
    }
  },
  "npmClient": "npm",
  "packages": ["packages/*", "packages/ngx-components/"],
  "useNx": true,
  "useWorkspaces": true,
  "version": "1970.01.01-alpha.000000"
}

  },
  "engines": {
    "node": ">=16",
    "yarn": "Use npm"
  },
  "exports": {
    ".": {
      "import": "./dist/react-components.mjs",
      "require": "./dist/react-components.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "./dist/react-components.umd.js",
  "module": "dist/react-components.mjs",
  "name": "@busybox/react-components",
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom",
    "wired-elements": {
      "roughjs": "$roughjs"
    }
  },
  "peerDependencies": {
    "react": ">= 18",
    "react-dom": ">= 18"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "directory": "packages/react-components",
    "type": "git",
    "url": "git@github.com:davidNHK/busybox.git"
  },
  "scripts": {
    "analyze": "npm run build && source-map-explorer ./dist/*.mjs --no-border-checks",
    "build": "vite build",
    "build:storybook": "build-storybook",
    "storybook": "start-storybook -p 6006",
    "test": "cypress open --component -b chrome",
    "test:storybook": "test-storybook"
  },
  "sideEffects": [
    "./cypress/support/*.ts"
  ],
  "types": "./dist/out-tsc/src/index.d.ts",
  "version": "1.3.0"
}
